#!../output/nlc 

fib(n) := {
    @if(n < 3){
        @return (1\1, 1\1,);
    };
    p := fib(n-1);
    @return (p[0] + p[1], p[0],);
};

calc(n) := {
    result := fib(n);
    $value := '{1}'( result[1] );
    print('%d: %d\n%s\n', $n, len($value), $value);
};

@timeit( calc(100) );

# и за 0.3 секунды на неторопливой машине для баловства получил 11494 знака числа Фибоначчи номер 55,000. 
