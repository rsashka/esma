#!../output/nlc 

fib(n) ::= {
    @if(n < 3){
        @return (1\1, 1\1,);
    }
    p ::= fib(n-1);
    @return ((p[1] + p[2]), p[1],);
};

calc(n) ::= {
    $v ::= '$1'(fib(n)[1]);
    print('%d: %d\n%s\n', $n, len($v), $v);
};

@time( calc(55_000) );

# и за 0.3 секунды на неторопливой машине для баловства получил 11494 знака числа Фибоначчи номер 55,000. 
